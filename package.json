{
  "name": "bootstrap-nucleus",
  "version": "1.0.0",
  "description": "Bootstrap v4 Nucleus - the best parts of Bootstrap.",
  "main": "index.js",
  "scripts": {
    "build": "node-sass --output-style expanded --precision 6 scss/nucleus.scss dist/bootstrap-nucleus.css",
    "build-min": "node-sass --output-style compressed --precision 6 scss/nucleus.scss dist/bootstrap-nucleus.min.css",
    "grid": "node-sass --output-style expanded --precision 6 scss/grid.scss css/grid.css",
    "reboot": "node-sass --output-style expanded --precision 6 scss/reboot.scss css/reboot.css",
    "utils": "node-sass --output-style expanded --precision 6 scss/utils.scss css/utils.css"
  },
  "keywords": [
    "Bootstrap",
    "CSS"
  ],
  "author": "Bootstrap, Christopher Murphy",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "node-sass": "^4.5.3"
  },
  "dependencies": {}
}
